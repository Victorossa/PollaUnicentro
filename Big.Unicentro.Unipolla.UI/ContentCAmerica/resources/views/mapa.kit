<!-- @include _header.kit -->
<header>

</header>


<div class="container">
  <div class="content-home">
    <!-- Start Header -->
    <div class="wrapper-general">
      <img class="logo-mapa" src="resources/assets/images/logo.png" alt="">
      <div class="wrapper">
        <div class="contentBody">
          <section class="polla">
            <div id="bracket">
              <div class="container">
                <div class="split split-one">
                  <div class="round round-two">
                    <div class="round-details purple">Cuartos de Finales</div>
                    <div class="flex-mobile">
                      <ul class="matchup matchup-blue">
                        <li class="team team-top"><input type="text" class="name" placeholder="A 1"><input type="text" class="score" value="0" disabled></li>
                        <span class="vs">VS</span>
                        <li class="team team-bottom"><input type="text" class="name" placeholder="3º B/C"><input type="text" class="score" value="0" disabled></li>
                      </ul>
                      <ul class="matchup matchup-blue">
                        <li class="team team-top"><input type="text" class="name" placeholder="2 A"><input type="text" class="score" value="0" disabled></li>
                        <span class="vs">VS</span>
                        <li class="team team-bottom"><input type="text" class="name" placeholder="2 B"><input type="text" class="score" value="0" disabled></li>
                      </ul>
                    </div>

                  </div> <!-- END ROUND TWO -->

                  <div class="round round-three">
                    <div class="round-details green">Semifinal</div>
                    <div class="flex-mobile">
                      <ul class="matchup matchup-red">
                        <li class="team team-top"><input type="text" class="name" placeholder="G 9"><input type="text" class="score" value="0" disabled></li>
                        <span class="vs">VS</span>
                        <li class="team team-bottom"><input type="text" class="name" placeholder="G 10"><input type="text" class="score" value="0" disabled></li>
                      </ul>
                    </div>
                  </div> <!-- END ROUND THREE -->
                </div>
                <div class="champion">


                  <div class="final">

                    <div class="round-details yellow">FINAL</div>
                    <ul class="matchup matchup-red">
                      <div class="top">

                      </div>
                      <li class="team team-top"><input type="text" class="name" placeholder="G  13"><input type="text" class="score" value="0" disabled></li>
                      <span class="vs">VS</span>
                      <li class="team team-bottom"><input type="text" class="name" placeholder="G 14"><input type="text" class="score" value="0" disabled></li>
                      <li class="team team-bottom">
                        <select class="name" name="">
                          <option value="">-- Seleccione --</option>
                        </select>
                        <input type="text" class="score" value="0" disabled>
                      </li>
                      <!--<input type="text" class="name-champ" placeholder="CAMPEÓN">-->
                    </ul>
                  </div>
                  <div class="semifinal">

                    <div class="round-details blue">TERCER LUGAR</div>
                    <ul class="matchup matchup-red">
                      <div class="top">

                      </div>
                      <li class="team team-top"><input type="text" class="name" placeholder="P 13"><input type="text" class="score" value="0" disabled></li>
                      <span class="vs">VS</span>
                      <li class="team team-bottom"><input type="text" class="name" placeholder="P 14"><input type="text" class="score" value="0" disabled></li>
                      <li class="team team-bottom">
                        <select class="name" name="">
                          <option value="">-- Seleccione --</option>
                        </select>
                        <input type="text" class="score" value="0" disabled>
                      </li>
                    </ul>

                  </div>
                  <div class="ct-cta hidden-max450">
                    <a class="cta" href="">ENVIAR</a>
                  </div>
                </div>
                <div class="split split-two">
                  <div class="round round-three">
                    <div class="round-details green">Semifinal</div>
                    <div class="flex-mobile">
                      <ul class="matchup matchup-red">
                        <li class="team team-top"><input type="text" class="name" placeholder="G 11"><input type="text" class="score" value="0" disabled></li>
                        <span class="vs">VS</span>
                        <li class="team team-bottom"><input type="text" class="name" placeholder="G 12"><input type="text" class="score" value="0" disabled></li>
                      </ul>
                    </div>
                  </div> <!-- END ROUND THREE -->
                  <div class="round round-two">
                    <div class="round-details purple">Cuartos de Finales</div>
                    <div class="flex-mobile">
                      <ul class="matchup matchup-blue">
                        <li class="team team-top"><input type="text" class="name" placeholder="1 B"><input type="text" class="score" value="0" disabled></li>
                        <span class="vs">VS</span>
                        <li class="team team-bottom"><input type="text" class="name" placeholder="2 C"><input type="text" class="score" value="0" disabled></li>
                      </ul>
                      <ul class="matchup matchup-blue">
